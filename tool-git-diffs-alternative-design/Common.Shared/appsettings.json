  "Commits": {
    "Patterns": [
      {
        "Regex": "(FEAT)-\\d{3,4}",
        "Priority": 1,
        "Header": "Features"
      },
      {
        "Regex": "(DEV)-\\d{3,4}",
        "Priority": 2,
        "Header": "Features"
      },
      {
        "Regex": "(DEFECTS)-\\d{3,4}",
        "Priority": 2,
        "Header": "Defects"
      }
    ],
    "SortOrder": [ "Features", "Defects", "Changes" ]
  }
}